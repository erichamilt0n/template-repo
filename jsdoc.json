{
    "source": {
        "include": ["src", "README.md"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "destination": "./docs",
        "recurse": true,
        "readme": "README.md"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}
